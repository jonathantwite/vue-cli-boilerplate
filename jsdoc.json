{
    "source": {
        "include": ["./src"],
        "includePattern": ".(js|vue)$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-vuejs"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./readme.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "zzz-template": "node_modules/better-docs"
    }
}